{
  "name": "prismjs-terminal",
  "version": "1.2.3",
  "description": "PrismJS syntax highlighting for the terminal",
  "keywords": [
    "terminal",
    "code",
    "syntax",
    "highlight",
    "highlighter",
    "highlighting",
    "hilite",
    "cli"
  ],
  "author": "Isaac Z. Schlueter <i@izs.me> (https://blog.izs.me)",
  "type": "module",
  "module": "./dist/esm/index.js",
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./themes": "./src/themes/index.ts"
    },
    "selfLink": false
  },
  "files": [
    "dist"
  ],
  "license": "BlueOak-1.0.0",
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --log-level warn",
    "typedoc": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/prismjs": "^1.26.5",
    "prettier": "^2.8.6",
    "tap": "^21.1.6",
    "tshy": "^3.1.0",
    "typedoc": "^0.28.14"
  },
  "engines": {
    "node": ">=16"
  },
  "funding": {
    "url": "https://github.com/sponsors/isaacs"
  },
  "repository": "https://github.com/isaacs/prismjs-terminal",
  "dependencies": {
    "chalk": "^5.2.0",
    "prismjs": "^1.30.0",
    "string-length": "^6.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./themes": {
      "import": {
        "types": "./dist/esm/themes/index.d.ts",
        "default": "./dist/esm/themes/index.js"
      },
      "require": {
        "types": "./dist/commonjs/themes/index.d.ts",
        "default": "./dist/commonjs/themes/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts"
}
